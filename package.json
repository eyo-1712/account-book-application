{
  "name": "application",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "format": "prettier --check --cache .",
    "format:fix": "prettier --write --cache .",
    "preview": "vite preview",
    "prepare": "husky",
