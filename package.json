    "husky": "^9.1.7",
    "postcss": "^8.4.49",
    "prettier": "^3.4.1",
    "storybook": "^8.4.6",
    "tailwindcss": "^3.4.15",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.15.0",
